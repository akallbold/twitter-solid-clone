{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/prop-types": 0,
    "consistent-return": 1,
    "react/no-unescaped-entities": 1,
    "react/react-in-jsx-scope": 0
  }
}